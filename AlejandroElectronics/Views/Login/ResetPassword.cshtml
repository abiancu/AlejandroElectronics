
@{
    ViewData["Title"] = "ResetPassword";
}



<form method="post">
    <div class="form-group">
        <div class="row">
            <div class="col-sm-8">
                <input type="password" name="password" class="form-control" required />
            </div>
        </div>
        <input type="submit" value="Reset your password" class="btn btn-dark" />
    </div>
</form>
        
       
       
       

